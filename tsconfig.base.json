{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "es5",
    "module": "es2020",
    "typeRoots": [
      "src/libs/**/*.d.ts",
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "data-grid": [
        "src/app/modules/data-grid/index"
      ],
      "themes": [
        "src/app/modules/themes/index"
      ],
      "core": [
        "src/app/modules/core/index"
      ],
      "order-form": [
        "src/app/modules/order-form/index"
      ],
      "storage": [
        "src/app/modules/storage/index"
      ],
      "ui": [
        "src/app/modules/ui/index"
      ],
      "notifier": [
        "projects/notifier/index"
      ],
      "trading-ui": [
        "src/app/modules/trading-ui/index"
      ],
      "lazy-modules": [
        "src/app/modules/lazy-modules/index"
      ],
      "lazy-assets": [
        "src/app/modules/lazy-assets/index"
      ],
      "communication": [
        "projects/communication/index"
      ],
      "fake-communication": [
        "src/app/modules/fake-communication/index"
      ],
      "layout": [
        "src/app/modules/layout/index"
      ],
      "search-select": [
        "projects/search-select/index"
      ],
      "base-components": [
        "projects/base-components/index"
      ],
      "trading": [
        "src/app/modules/trading/index"
      ]
    }
  }
}