{
  "name": "angular-starter",
  "version": "10.0.5",
  "description": "TradingChart",
  "author": {
    "email": "tech@avidi.tech",
    "name": "tech"
  },
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "npm run font && ng build",
    "build-prod": "ng build --prod",
    "pack": "electron-builder --dir",
    "dist:win": "electron-builder --win --x64",
    "dist:linux": "electron-builder --linux",
    "postinstall": "electron-builder install-app-deps",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --base-href && electron .",
    "dev:ssr": "ng run angular-starter:serve-ssr",
    "serve:ssr": "node dist/angular-starter/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-starter:server:production",
    "electron:build": "npm run build:prod && electron-builder build",
    "prerender": "ng run angular-starter:prerender",
    "stats": "ng build --stats-json",
    "font": "node icon-generator.js",
    "analyze": "webpack-bundle-analyzer dist/browser/stats.json",
    "check-bundle": "npm run stats && npm run analyze"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ganatan/angular-example-ssr.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.8",
    "@angular/cdk": "10.1.2",
    "@angular/common": "10.0.8",
    "@angular/compiler": "10.0.8",
    "@angular/core": "10.0.8",
    "@angular/forms": "10.0.8",
    "@angular/platform-browser": "10.0.8",
    "@angular/platform-browser-dynamic": "10.0.8",
    "@angular/platform-server": "10.0.8",
    "@angular/router": "10.0.8",
    "@fortawesome/fontawesome-free": "5.14.0",
    "@ngneat/until-destroy": "8.0.1",
    "@nguniversal/express-engine": "10.0.1",
    "electron-log": "^4.2.4",
    "express": "4.17.1",
    "ng-packagr": "^10.1.2",
    "ng-zorro-antd": "9.3.0",
    "ngx-monaco-editor": "9.0.0",
    "rxjs": "6.6.2",
    "svgtofont": "^2.1.0",
    "tslib": "2.0.1",
    "underscore": "1.10.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.5",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular/cli": "10.0.5",
    "@angular/compiler-cli": "10.0.8",
    "@nguniversal/builders": "10.0.1",
    "@types/express": "4.17.7",
    "@types/jasmine": "3.5.12",
    "@types/jasminewd2": "2.0.8",
    "@types/moment": "^2.13.0",
    "app-builder-bin": "^3.5.10",
    "codelyzer": "6.0.0",
    "electron": "10.1.3",
    "electron-builder": "19.56.2",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "15.1.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "^10.0.0",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "build": {
    "appId": "angular-starter",
    "productName": "TradingChart",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "deb",
      "category": "Utility"
    }
  }
}
